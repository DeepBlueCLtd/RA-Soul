{
  "name": "ra-soul",
  "private": false,
  "author": {
    "name": "Ian Mayo"
  },
  "contributors": [
    {
      "name": "Ian Mayo"
    }
    ],
  "description": "Data and Auth Providers to allow Soul to be used as backend for React-Admin",
  "version": "1.0.0",
  "keywords": [
    "react-admin",
    "soul",
    "data-provider",
    "auth-provider"
  ],
  "license": "EPL-2.0",
  "type": "module",
  "scripts": {
    "start": "npx soul-cli soul -d ./db/chinook.db -p $PORT -e /app/_extensions",
    "dev:soul": "dotenv -- cross-var soul -d ./db/chinook.db -p %PORT% -e %EXTENSIONS% -c http://localhost:5173,http://127.0.0.1:5173 -a --atet=10M --rtet=30M --ts=ABCDEF --iuu=john --iup=AX34%V1!ct2",
    "dev:client": "vite",
    "dev:soul-client": "dotenv -- cross-var soul -d ./db/chinook.db -p %PORT% -e %EXTENSIONS%",
    "build": "vite build && cp -R dist _extensions",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "jsdoc": "^4.0.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-admin": "^4.16.18",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "soul-cli": "^0.7.8",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.2.1",
    "vite": "^3.2.3"
  },
  "bugs": {
    "url": "https://github.com/DeepBlueCLtd/RA-Soul/issues"
  },
  "homepage": "https://github.com/DeepBlueCLtd/RA-Soul"
}
