{
  "name": "RCO-Soul",
  "version": "1.0.0",
  "description": "RCO client connection to a Soul backend",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:soul & npm run start:client",
    "dev": "npm run dev:soul & npm run start:client",
    "start:soul": "npx soul-cli soul -d ./db/chinook.db -p $PORT",
    "dev:soul": "npx soul-cli soul -d ./db/chinook.db -p 9000",
    "start:client": "cd client && npm run dev",
    "heroku-prebuild": "cd client && npm install"
  },
  "engines": {
    "node": "18.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeepBlueCLtd/RCO-Soul.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeepBlueCLtd/RCO-Soul/issues"
  },
  "homepage": "https://github.com/DeepBlueCLtd/RCO-Soul#readme",
  "dependencies": {
    "express": "^4.18.2",
    "soul-cli": "^0.3.4",
    "vite": "^4.3.9"
  }
}
