{
  "name": "RCO-Soul",
  "version": "1.0.0",
  "description": "RCO client connection to a Soul backend",
  "main": "index.js",
  "scripts": {
    "preinstall": "cd client && npx vite && && npm install && npm run build && cp -R dist ../_extensions",
